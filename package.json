{
  "name": "urf",
  "version": "1.1.4",
  "description": "URF Stream Graphics Bundle",
  "homepage": "https://urfonline.com",
  "author": {
    "name": "Henry Le Grys",
    "email": "tech@urfonline.com",
    "url": "https://offbeatwit.ch"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "mount": [
      {
        "directory": "vendor",
        "endpoint": "vendor"
      }
    ],
    "dashboardPanels": [
      {
        "name": "urf",
        "title": "URF",
        "width": 4,
        "file": "urf.html",
        "headerColor": "b02727"
      },
      {
        "name": "urf-jingles",
        "title": "Jingles",
        "width": 4,
        "file": "jingles.html",
        "headerColor": "daa520"
      },
      {
        "name": "urf-obs-control",
        "title": "OBS",
        "width": 2,
        "file": "obs.html",
        "headerColor": "008b8b"
      },
      {
        "name": "urf-atem-switcher",
        "title": "Video Mixer",
        "width": 2,
        "file": "atem.html",
        "headerColor": "008b8b"
      }
    ],
    "assetCategories": [
      {
        "name": "jingles",
        "title": "Memes & Jingles",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "png"
        ]
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "nowplaying.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "charity_index.html",
        "width": 1280,
        "height": 720
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT",
  "dependencies": {
    "atem-connection": "^0.9.0",
    "got": "^9.3.2",
    "lastfm": "^0.9.3",
    "obs-websocket-js": "^1.2.0"
  }
}
